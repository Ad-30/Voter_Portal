<body class="text-center">

  <main class="form-signin w-100 m-auto">
    <form action="/login" method="POST" class="signin">


      <h1 class="h3 mb-3 fw-normal">Login</h1>



      <div class="form-floating" >
        <input name="username" type="email" class="form-control bottom" id="floatingInput2 username" autocomplete="username" placeholder="E-mail" required autofocus>
        <label for="username">E-mail</label>
      </div>

      <div class="form-floating" style="position:relative;top:10px;">
        <input name="password" type="password" class="form-control bottom" id="floatingInput3 current-password" placeholder="Password" autocomplete="current-password" required>
        <label for="current-password">Password</label>
      </div>




      <button class="w-100 btn btn-lg btn-primary" type="submit" style="position:relative;top:20px;">Login</button>
      <a href="/signup" style="position:relative;top:20px;">Signup?</a>
      <p class="mt-5 mb-3 text-muted">&copy; 2023</p>
    </form>
  </main>


