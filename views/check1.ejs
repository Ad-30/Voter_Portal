  <%- include("partials/header");-%>

  <%data.forEach(function(data){%>
    <%if (id === data._id.toString().replace(/[^a-z0-9]/g, '')) {%>
  <div class="row">
    <div class="col-lg-6">
      <p>First name : <%=data.firstName %></p>
    </div>
    <div class="col-lg-6">
     <p>Last name : <%=data.lastName %></p>
    </div>
    <div class="col-lg-6">
      <p>Father's name : <%=data.fathersName %></p>
    </div>
    <div class="col-lg-6">
      <%  var date = data.dob;
      var year = date.toLocaleString("default", { year: "numeric" });
      var month = date.toLocaleString("default", { month: "2-digit" });
      var day = date.toLocaleString("default", { day: "2-digit" });
      var formattedDate = day + "-" + month + "-" + year;%>
      <p>D.O.B name : <%= formattedDate %></p>
    </div>
    <div class="col-lg-6">
      <p>E-mail : <%=data.email %></p>
    </div>
    <div class="col-lg-6">
      <p>mobile no.  : <%=data.mob %></p>
    </div>
    <div class="col-lg-6">
      <p>Address  : <%=data.address.house_no %></p>
    </div>
    <div class="col-lg-6">
      <p>locality  : <%=data.address.locality %></p>
    </div>
    <div class="col-lg-6">
      <p>road name : <%=data.address.road_name %></p>
    </div>
    <div class="col-lg-6">
      <p>Pincode  : <%=data.address.pincode %></p>
    </div>
    <div class="col-lg-6">
      <p>State  : <%=data.address.state %></p>
    </div>
    <div class="col-lg-6">
      <p>District  : <%=data.address.district %></p>
    </div>
    <div class="col-lg-6">
      <p>Identity type  : <%=data.identity_type %></p>
    </div>
    <div class="col-lg-6">
      <p>Identity number  : <%=data.identity_no %></p>
    </div>
    <div class="division-submit"><a href="/application"><input class="btn btn-primary btn1"type="submit" name="" value="Submit"></a>
  </div
  </div>
  <%}%>

  <%})%>




  <%- include("partials/footer");-%>
